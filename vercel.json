{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "npm run prisma:deploy && npm run build",
  "installCommand": "npm ci",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- . ':!README.md'",
  "functions": {
    "api/**": {
      "maxDuration": 30
    }
  }
}
